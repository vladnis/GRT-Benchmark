<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.licenta.grt_benchmark"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <!-- Write permission for tracing files -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/chart"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.licenta.grt_benchmark.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="runTestsActivity.RunTests"
            android:label="@string/title_activity_run_tests" >
        </activity>
        <activity
            android:name="manageDatasets.ManageDatasetsActivity"
            android:label="@string/title_activity_manage_datasets_acticity" >
        </activity>
        <activity
            android:name="manageDatasets.CreateDatasetActivity"
            android:label="@string/title_activity_create_dataset" >
        </activity>
        <activity
            android:name="manageDatasets.ShowDrawing"
            android:label="@string/title_activity_show_drawing" >
        </activity>
    </application>

</manifest>
